{
  "snippet": {
    "prefix": ["au"],
    "body": [ "autocmd $TM_SELECTED_TEXT" ],
    "description": "Example for selected text"
  },
  "brace": {
    "prefix": ["brace"],
    "body": ["{$TM_SELECTED_TEXT$1}$2"]
  },
  "paren": {
    "prefix": ["paren"],
    "body": ["($TM_SELECTED_TEXT$1)$2"]
  },
  "tag": {
    "prefix": ["tag"],
    "body": ["<$1>$TM_SELECTED_TEXT$2</$1>$3"]
  },
  "in quotes": {
    "prefix": "inq",
    "body": "'$TM_SELECTED_TEXT$1'$2"
  },
  "log": {
    "prefix": ["log"],
    "body": "console.log($TM_SELECTED_TEXT$1);$2"
  }
}
