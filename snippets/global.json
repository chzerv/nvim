{
  "snippet": {
    "prefix": ["au"],
    "body": [ "autocmd $TM_SELECTED_TEXT" ],
    "description": "Example for selected text"
  },
  "brace": {
    "prefix": ["brace"],
    "body": ["\\{${1:$TM_SELECTED_TEXT}\\}"]
  },
  "paren": {
    "prefix": ["paren"],
    "body": ["(${1:$TM_SELECTED_TEXT})"]
  },
  "tag": {
    "prefix": ["tag"],
    "body": ["<$1>${2:$TM_SELECTED_TEXT}</$1>"]
  }

}
